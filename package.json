{
  "name": "jest-cucumber-examples",
  "version": "3.0.1",
  "description": "Execute Gherkin scenarios in Jest",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "jest": "jest --verbose",
    "test": "npm run build & jest --color"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bencompton/jest-cucumber.git"
  },
  "keywords": [
    "jest",
    "gherkin",
    "cucumber"
  ],
  "author": "vicboma1",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vicboma1"
  },
  "homepage": "https://github.com/bencompton/jest-cucumber#readme",
  "devDependencies": {
    "babel-jest": "^24.1.0",
    "jest": "^26.6.3",
    "jest-cucumber": "^3.0.1",
    "ts-jest": "^26.5.3",
    "tslint": "^5.10.0",
    "typescript": "^3.9.9",
    "typescript-jest": "0.0.0"
  },
  "dependencies": {
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.7",
    "@types/node": "^11.15.48",
    "callsites": "^3.0.0",
    "gherkin": "^9.0.0",
    "glob": "^7.1.6",
    "uuid": "^8.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.steps.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ]
  }
}
